    <%- include('../includes/navigation.ejs') %>
    <main>
        <% if (orders.length <= 0) { %>
            <h1>Working on it..</h1>
        <% } else { %>
            <ul>
                <% orders.forEach(order => { %>
                    <li>
                        <h1>
                            # <%= order.id %> 
                        </h1>
                        <ul>
                            <% order.products.forEach(product => { %>
                                <li><%= item.title %> (<%= product.orderItem.quantity %>) </li>
                            <% }) %>
                        </ul>
                    </li>
                <% }); %>
            </ul>
        <% } %>
            

    </main>
    <%- include('../includes/end.ejs') %>